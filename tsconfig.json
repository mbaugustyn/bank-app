{
    "compilerOptions": {
        "module": "ES2022",
        "sourceMap": true,
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "bundler"
    },
    "include": [
        "src/*/*",
    ],
    "exclude": [
        "node_modules",
    ],
}
